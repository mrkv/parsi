<table class="uk-table uk-table-hover uk-table-divider">
  <thead>
    <tr>
      <th>URL</th>
      <th>Title</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% pages.each do |page| %>
      <tr id="books">
        <td><%= page.url %></td>
        <td><%= page.title %></td>
        <td>
          <%= status_label(page) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
